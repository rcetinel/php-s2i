#!/bin/bash

mkdir ./gitrepo 
echo "Before assembling"

cd ./gitrepo
git clone -v --progress https://github.com/rcetinel/html 

echo "First clone process is OK" 
mkdir ./privaterepo
echo "Priv repo clone will be started "
git clone -v --progress https://github.com/linuxera-tr/Scripts


/usr/libexec/s2i/assemble
rc=$?

if [ $rc -eq 0 ]; then
    echo "After successful assembling"
else
    echo "After failed assembling"
fi
exit $rc


